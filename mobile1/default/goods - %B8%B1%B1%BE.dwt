        case 'detail-review' :
		  $("#detail_nav li").removeClass("current");
          $("#detail_nav li").eq(1).addClass("current");
          break;
        case 'detail-recommend' :
		  $("#detail_nav li").removeClass("current");
		  $("#detail_nav li").eq(2).addClass("current");
