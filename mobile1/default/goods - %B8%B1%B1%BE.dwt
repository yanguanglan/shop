		 // $("#detail_nav li:eq(0)").addClass("current");
          break;
      }
	$("#detail_nav li").click(function(){
		    i = $(this).index('#detail_nav li');
			$("#detail_nav li").removeClass("current");
		    $("#detail_nav li").eq(i).addClass("current");
			
