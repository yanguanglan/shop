$(function(){

	//alert(document.URL);
	arg=document.URL.split("#");
	//alert( arg[1] );
    switch(arg[1])
      {
        case 'detail-desc' :
		  $("#detail_nav li").removeClass("current");
          $("#detail_nav li").eq(0).addClass("current");
